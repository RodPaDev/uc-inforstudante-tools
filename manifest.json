{
    "name": "InforStudante Tools",
    "version": "1.0",
    "description": "This extension allows you to download pdfs when no download link is avaliable",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "permissions": [
        "activeTab"
    ]
}